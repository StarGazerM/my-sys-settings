{
	"name": "line-counter",
	"displayName": "line-counter",
	"description": "Count the lines of code(LOC).",
	"icon": "images/icon.png",
	"version": "0.5.2",
	"publisher": "dollyn",
	"license": "MIT",
	"engines": {
		"vscode": "^1.18.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.count-file",
		"onCommand:extension.count-workspace"
	],
	"main": "./out/src/extension",
	"contributes": {
		"commands": [
			{
				"command": "extension.count-file",
				"title": "Count Current File"
			},
			{
				"command": "extension.count-workspace",
				"title": "Count Workspace"
			}
		],
		"configuration": {
			"title": "Line-Counter Configurations",
			"properties": {
				"line-counter.excludes": {
					"type": "array",
					"default": [
						"**/.vscode/**"
					],
					"description": "Files excluded from counting."
				},
				"line-counter.includes": {
					"type": "array",
					"default": [
						"**/*"
					]
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "tsc -p ./",
		"compile": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^2.5.0",
		"vscode": "^1.1.0",
		"mocha": "^2.3.3",
		"@types/node": "^6.0.40",
		"@types/mocha": "^2.2.32"
	},
	"__metadata": {
		"id": "d002d11c-7c09-4497-9da9-093798ae0ded",
		"publisherId": "26519c49-6d63-4fb5-9c23-b886e6e2ebb8",
		"publisherDisplayName": "dollyn"
	}
}